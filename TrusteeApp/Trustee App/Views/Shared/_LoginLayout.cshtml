<!DOCTYPE html>

<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Trustee Portal - @ViewData["Title"]</title>

    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;600&amp;display=swap" rel="stylesheet">

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-icons/1.9.1/font/bootstrap-icons.min.css"
          integrity="sha512-5PV92qsds/16vyYIJo3T/As4m2d8b6oWYfoqV+vtizRB6KhF1F9kYzWzQmsO6T3z3QG2Xdhrx7FQ+5R1LiQdUA=="
          crossorigin="anonymous"
          referrerpolicy="no-referrer" />


    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">


    @*<link rel="stylesheet" href="~/css/table-filter-style.css" />*@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    @*<link href="~/css/login.css" asp-append-version="true" rel="stylesheet" />*@

    @*<link rel="stylesheet" href="~/fontawesome/css/all.css" />*@
    @*<link rel="stylesheet" href="~/fontawesome-free-5.15.4-web/css/all.css" />*@
    <link href="~/css/mysite.css" asp-append-version="true" rel="stylesheet" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.css" integrity="sha512-3pIirOrwegjM6erE5gPSwkUzO+3cTjpnV9lexlNZqvupR64iZBnOOTiiLPb9M36zpMScbmUNIcHUqKD47M719g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />

</head>


<body>

    @*<vc:preloader />*@

    <partial name="_BusyIndicatorPartial" />

    <div class="d-flex justify-content-end col-12 m-3 pe-5 position-fixed" style="">
        <nav class="">
            <div class="d-flex align-content-center">

                <a asp-controller="" asp-action="" class="me-4 trustee-btn">
                    <div>
                        <i class="bi bi-globe2 fs-2 text-white shadow-lg"></i>
                    </div>
                </a>
            </div>
        </nav>
    </div>

    <nav class="ps-3 ms-md-5 ps-md-3 pt-4 position-fixed">
        <a class="navbar-brand shadow-lg" asp-controller="Home" asp-action="Index">
            <h3 class="page-header-title trustee-header text-uppercase">
                Trustee Portal
            </h3>
        </a>
    </nav>

    <div id="content-placeholder" class="container-fluid p-0" style="background: rgb(212,54,98); background: linear-gradient(133deg, rgba(212,54,98,1) 0%, rgba(255,252,253,1) 100%);">

        <partial name="_Notifications" />

        <div class="pt-5 px-5 px-md-0 pt-md-0 render-div" style="min-height: 100vh;">
            @RenderBody()
        </div>
    </div>

    @if (IsSectionDefined("Component"))
    {
        @RenderSection("Component", false)
    }

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"
            asp-fallback-src="~/lib/jquery/dist/jquery.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.9.2/umd/popper.min.js" integrity="sha512-2rNj2KJ+D8s1ceNasTIex6z4HWyOnEYLVC3FigGOmyQCZc2eBXKgOxQmo3oKLHyfcj53uz4QMsRCWNbLd32Q1g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.js"></script>

    @*<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>*@

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>